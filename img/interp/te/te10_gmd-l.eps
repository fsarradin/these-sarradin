/mkBox {
  /yparam exch def
  /xparam exch def

  /scx xparam 3 get def
  /xmin xparam 0 get 0 min scx mul def
  /xmax xparam 1 get scx mul def
  /xstep xparam 2 get scx mul def

  /scy yparam 3 get def
  /ymin yparam 0 get 0 min scy mul def
  /ymax yparam 1 get scy mul def
  /ystep yparam 2 get scy mul def

  /drawLine {
    % dx, dy, x, y
    newpath
    moveto
    rlineto
    stroke
  } def

  currentlinewidth currentdash
%  2 setlinewidth
%  newpath
%  xmin ymin moveto
%  xmin ymax lineto
%  xmax ymax lineto
%  xmax ymin lineto
%  closepath stroke

  0.5 setlinewidth
  [2 2] 0 setdash

  xparam 0 get 0 min  % min
  xparam 2 get        % step
  xparam 1 get {      % max
    dup 0 eq /org exch def
    dup scx mul
      dup                              % x
      0 exch                           % dx
      ymax ymin sub 5 scy mul add exch % dy
      -3 scy mul                       % y
    org {
      1 setlinewidth [] 0 setdash
      drawLine
      0.5 setlinewidth [2 2] 0 setdash
     } {
       drawLine
     } ifelse
    -25 moveto cshowint
  } for

  yparam 0 get 0 min  % min
  yparam 2 get        % step
  yparam 1 get {      % max
    dup 0 eq /org exch def
    dup scy mul 0 0 ymin min sub add
      dup                      % y
      xmax xmin sub 5 add exch % dx
      0 exch                   % dy
      -5 exch                  % x
     org {
      1 setlinewidth [] 0 setdash
      drawLine
      0.5 setlinewidth [2 2] 0 setdash
     } {
       drawLine
     } ifelse
    3 sub xmin 10 sub exch moveto rshowint
  } for

  currentfont
  /Helvetica-Bold-iso findfont 14 scalefont setfont
  xparam 4 get dup stringwidth pop 2 div
  xmax xmin sub 2 div xmin add exch sub
  -42 moveto
  show

  yparam 4 get dup stringwidth pop 2 div
  ymax ymin add 2 div 0 0 ymin min sub add exch sub
  xmin 40 sub exch moveto
  gsave 90 rotate show grestore
  setfont

  setdash setlinewidth
} def

